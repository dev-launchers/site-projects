{
  "extends": ["airbnb", "airbnb-typescript", "next", "prettier"],
  "rules": {
    "array-callback-return": "off",
    "no-plusplus": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off"
  },
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": { "browser": true, "node": true, "es2021": true }
}
